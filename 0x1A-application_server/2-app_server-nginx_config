server {
    # Listen on port 80 for incoming HTTP requests
    listen 80 default_server;
    listen [::]:80 default_server;
    
    # Server name for this block
    server_name 100.25.102.204;
    
    # Add a custom header to identify the serving server
    add_header X-Served-By 430003-web-01;

    # Define a location block for the /airbnb-onepage/ route
    location /airbnb-onepage/ {
        # Proxy requests to the process listening on port 5000
        proxy_pass http://127.0.0.1:5000/airbnb-onepage;
    }

    # Error page configuration
    error_page 404 /error.html;
    location /404 {
        root /var/www/html;
        internal;
    }
}